{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "WAFResult",
  "type": "object",
  "required": [
    "Timestamp",
    "Pillar",
    "ControlId",
    "ControlName",
    "SubscriptionId",
    "Status",
    "Score"
  ],
  "properties": {
    "Timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "Pillar": {
      "type": "string"
    },
    "ControlId": {
      "type": "string"
    },
    "ControlName": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "SubscriptionId": {
      "type": "string"
    },
    "ResourceId": {
      "type": "string"
    },
    "TestMethod": {
      "type": "string"
    },
    "Status": {
      "type": "string",
      "enum": [
        "Pass",
        "Warn",
        "Fail",
        "Manual"
      ]
    },
    "Score": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100
    },
    "Evidence": {
      "type": "string"
    },
    "Recommendation": {
      "type": "string"
    },
    "EstimatedROI": {
      "type": [
        "string",
        "null"
      ]
    }
  }
}
